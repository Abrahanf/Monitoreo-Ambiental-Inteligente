# backend/.env.example
# Flask
FLASK_APP=run.py
FLASK_ENV=development
SECRET_KEY=your-super-secret-key-change-this

# Database
DATABASE_URL=mysql+pymysql://admin:password@your-rds-endpoint:3306/environmental_db

# JWT
JWT_SECRET_KEY=your-jwt-secret-key

# MQTT
MQTT_BROKER_URL=your-iot-core-endpoint.iot.us-east-1.amazonaws.com
MQTT_BROKER_PORT=8883
MQTT_USERNAME=
MQTT_PASSWORD=

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
S3_BUCKET=environmental-reports-bucket

# IA Service
IA_SERVICE_URL=http://ia-service:8001
